        RUN+="/bin/bash -c 'chgrp -R $env{SUBSYSTEM} %N %S/$env{DEVPATH} %S/class/vpu;chmod g+w -R %N %S/$env{DEVPATH} %S/class/vpu'"
SUBSYSTEM=="vpu_clk_dev", ACTION=="add", \
        RUN+="/bin/bash -c 'chgrp -R vpu %N %S/$env{DEVPATH} %S/class/vpu_clk_dev;chmod g+w -R %N %S/$env{DEVPATH} %S/class/vpu_clk_dev'"
SUBSYSTEM=="platform", DRIVER=="hb_vpu", ACTION=="add", \
        RUN+="/bin/bash -c 'chgrp vpu %S/devices/platform/soc/a8000000.vpu/fps;chmod g+w %S/devices/platform/soc/a8000000.vpu/fps'"

