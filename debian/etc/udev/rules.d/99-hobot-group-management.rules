SUBSYSTEM=="gpio", ACTION=="add", \
        RUN+="/bin/bash -c 'chgrp -R $env{SUBSYSTEM} %N %S/$env{DEVPATH} %S/class/misc;chmod g+w %N %S/$env{DEVPATH} %S/class/gpio'"
SUBSYSTEM=="vps", ACTION=="add", \
        RUN+="/bin/bash -c 'chgrp -R $env{SUBSYSTEM} %N %S/$env{DEVPATH} %S/class/vps;chmod g+rw -R %N %S/$env{DEVPATH} %S/class/vps'"
SUBSYSTEM=="ipu", ACTION=="add", \
        RUN+="/bin/bash -c 'chgrp -R $env{SUBSYSTEM} %N %S/$env{DEVPATH} ;chmod g+w %N %S/$env{DEVPATH}'"
SUBSYSTEM=="misc", ACTION=="add", \
        RUN+="/bin/bash -c 'chgrp -R $env{SUBSYSTEM} %N %S/$env{DEVPATH} %S/class/misc;chmod g+w -R %N %S/$env{DEVPATH} %S/class/misc'"
SUBSYSTEM=="jpu", ACTION=="add", \
